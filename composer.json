{
    "name": "lorenzosapora/nova-action-button",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "authors": [
        {
            "name": "Lorenzo Sapora",
            "email": "lorenzo.sapora@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LorenzoSapora\\NovaActionButton\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LorenzoSapora\\NovaActionButton\\NovaActionFieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
